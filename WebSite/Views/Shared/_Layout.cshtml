<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="~/css/content-min.css?@ViewBag.Version" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css?@ViewBag.Version" />
    <link rel="stylesheet" href="~/css/materialize.min.css?@ViewBag.Version" />

    <script src="~/app/app.min.js?@ViewBag.Version" type="text/javascript"></script>
    <script src="~/lib/angular.min.js?@ViewBag.Version" type="text/javascript"></script>
    <script src="~/lib/angular-route.min.js?@ViewBag.Version" type="text/javascript"></script>
</head>
<body>
    <div class="container body-content">
        @RenderBody()
    </div>
    @RenderSection("scripts", false)
</body>
</html>
